{
    "name": "nutritious-server",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "yarn": "1.22.22",
        "npm": "10.2.0",
        "node": "20"
    },
    "main": "dist/src/index.js",
    "scripts": {
        "pretty": "prettier --write ./src",
        "pre-commit": "lint-staged",
        "lint-fix": "eslint --fix src/ --ext .ts",
        "lint": "eslint src/ --ext .ts",        
        "watch": "nodemon",
        "build": "tsc",
        "test": "jest --no-cache --detectOpenHandles --coverage --forceExit",
        "seed": "ts-node src/db/seed",
        "seed:get": "./get_csv_data.sh",
        "seed:latest": "./get_csv_data.sh && ts-node src/db/seed",
        "prune-db": "ts-node src/db/prune",
        "test-links": "ts-node src/db/util/brokenLinksFinder",        
        "docker:test": "docker exec -it nutritious-server sh -c 'yarn run test'",
        "docker:test-file": "docker exec -it nutritious-server sh -c 'yarn run test \"$0\"'",
        "docker:seed": "docker exec -it nutritious-server sh -c 'yarn run seed'",
        "docker:seed:get": "docker exec -it nutritious-server sh -c 'yarn run seed:get'",
        "docker:seed:latest": "docker exec -it nutritious-server sh -c 'yarn run seed:latest'",
        "docker:prune-db": "docker exec -it nutritious-server sh -c 'yarn run prune-db'",
        "docker:test-links": "docker exec -it nutritious-server sh -c 'yarn run test-links'",        
        "review": "yarn lint --fix && yarn pretty && yarn docker:test"        
    },
    "dependencies": {
        "@aws-sdk/client-ses": "^3.734.0",
	"aws-sdk": "^2.1692.0",
	"bcrypt": "^5.1.1",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "eslint-config-prettier": "^9.0.0",
        "express": "^4.18.2",
        "fuse.js": "^6.6.2",
        "helmet": "^7.0.0",
        "jest": "^29.7.0",
        "jsonwebtoken": "^9.0.2",
        "mongodb": "^6.1.0",
        "mongodb-memory-server": "^9.0.0",
        "multer": "^1.4.5-lts.1",
        "node-fetch": "3.3.2",
        "nodemailer": "^6.9.6",
        "nodemon": "^3.0.1",
        "papaparse": "^5.4.1",
        "pino": "^8.16.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cookie-parser": "^1.4.4",
        "@types/cors": "^2.8.14",
        "@types/express": "^4.17.19",
        "@types/jest": "^29.5.5",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/multer": "^1.4.8",
        "@types/node": "^20.8.4",
        "@types/node-fetch": "2.6.6",
        "@types/nodemailer": "^6.4.11",
        "@types/papaparse": "^5.3.9",
        "@typescript-eslint/eslint-plugin": "^6.7.5",
        "@typescript-eslint/parser": "^6.7.5",
        "eslint": "^8.51.0",
        "prettier": "^3.0.3",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.7.0",
        "typescript": "^5.2.2"
    }
}
